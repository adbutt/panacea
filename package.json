{
    "name": "panacea",
    "version": "1.0.0",
    "description": "base build WP",
    "main": "gulpfile.js",
    "author": "Adam Butterworth, workpower <adbutt@gmail.com>",
    "copyright": "workpower",
    "license": "UNLICENSED",
    "private": true,
    "paths": {
        "src": {
            "base": "./src/",
            "css": "./src/css/",
            "fontello": "./src/fontello/",
            "fonts": "./src/fonts/",
            "js": "./src/js/",
            "jsv": "./src/js/src",
            "img": "./src/img/",
            "scss": "./src/scss/"
        },
        "dist": {
            "base": "./public/",
            "css": "./public/css/",
            "js": "./public/js/",
            "fonts": "./public/fonts/",
            "img": "./public/img/"
        },
        "build": {
            "base": "./build/",
            "css": "./build/css/",
            "fontello": "./build/fonts/fontello/",
            "fonts": "./build/fonts/",
            "js": "./build/js/",
            "html": "./build/html/",
            "img": "./build/img/"
        },
        "favicon": {
            "src": "./src/img/favicon_src.png",
            "dest": "./public/img/site/",
            "path": "/img/site/"
        },
        "scss": [
            "node_modules/"
        ],
        "templates": "./template-parts/"
    },
    "urls": {
        "live": "https://wlwco.dev/",
        "local": "https://adamplay.wpm3dia.com/",
        "critical": "https://adamplay.wpm3dia.com/"
    },
    "vars": {
        "siteCssName": "site.combined.min.css",
        "scssName": "site.scss",
        "cssName": "site.css",
        "siteJsName": "site.combined.min.js"
    },
    "globs": {
        "distCss": [
            "./node_modules/normalize.css/normalize.css",
            "./build/fonts/fontello/css/panacea-codes.css",
            "./build/css/*.css",
            "./src/css/*.css"
        ],
        "img": [
            "./public/img/"
        ],
        "fonts": [
            "./build/fonts/fontello/font/*.{eot,ttf,woff,woff2}",
            "./src/fonts/*.{eot,ttf,woff,woff2}"
        ],
        "critical": [
            {
                "url": "",
                "template": "index"
            }
        ],
        "download": [
            {
                "url": "https://www.google-analytics.com/analytics.js",
                "dest": "./public/js/"
            },
            {
                "url": "https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js",
                "dest": "./public/js/"
            }
        ],
        "distJs": [
            "./build/js/*.js"
        ],
        "fullJs": [
            "./src/js/src/skip-link-focus-fix.js",
            "./node_modules/moveto/dist/moveTo.js",
            "./node_modules/what-input/dist/what-input.js",
            "./src/js/src/navigation.js",
            "./src/js/src/scripts.js"
        ],
        "babelJs": [
            "./src/js/src/*.js"
        ],
        "inlineJs": [
            "./node_modules/fontfaceobserver/fontfaceobserver.js",
            "./node_modules/tiny-cookie/tiny-cookie.min.js",
            "./src/js/inline/asyncload-site-fonts.js"
        ],
        "siteIcon": "./public/img/site/favicon.*"
    },
    "dependencies": {
        "flexboxgrid": "^6.3.1",
        "fontfaceobserver": "^2.0.5",
        "hamburgers": "^0.9.1",
        "jeet": "^7.2.0",
        "normalize.css": "^5.0.0",
        "tiny-cookie": "^1.0.1",
        "moveto": "^1.7.1",
        "what-input": "^5.0.4"
    },
    "devDependencies": {
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-preset-es2015": "^6.16.0",
        "chalk": "^1.1.3",
        "critical": "^1.1.0",
        "eslint": "^4.18.0",
        "eslint-config-standard": "^11.0.0-beta.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-node": "^6.0.0",
        "eslint-plugin-promise": "^3.6.0",
        "eslint-plugin-standard": "^3.0.1",
        "fancy-log": "^1.2.0",
        "git-rev-sync": "^1.7.1",
        "gulp": "^3.9.0",
        "gulp-autoprefixer": "^3.1.0",
        "gulp-babel": "^6.1.2",
        "gulp-cached": "^1.1.1",
        "gulp-concat": "^2.6.0",
        "gulp-cssnano": "^2.1.2",
        "gulp-debug": "^2.1.2",
        "gulp-download": "^0.0.1",
        "gulp-favicons": "^2.2.6",
        "gulp-filter": "^5.0.1",
        "gulp-fontello": "^0.4.6",
        "gulp-header": "^1.8.7",
        "gulp-if": "^2.0.1",
        "gulp-imagemin": "^3.1.1",
        "gulp-livereload": "^3.8.1",
        "gulp-load-plugins": "^1.3.0",
        "gulp-newer": "^1.2.0",
        "gulp-plumber": "^1.1.0",
        "gulp-print": "^2.0.1",
        "gulp-rename": "^1.2.2",
        "gulp-replace": "0.5.4",
        "gulp-rev": "^7.1.0",
        "gulp-sass": "^3.1.0",
        "gulp-size": "^2.1.0",
        "gulp-sourcemaps": "^2.2.1",
        "gulp-streamify": "1.0.2",
        "gulp-uglify": "^1.5.4",
        "moment": "^2.14.1",
        "pa11y": "^4.11.0",
        "vinyl-source-stream": "^1.1.0",
        "gulp-util": "^3.0.8"
    },
    "scripts": {
        "start": "gulp",
        "build": "gulp build"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:adbutt/panacea.git"
    },
    "bugs": {
        "email": "adbutt@gmail.com"
    }
}
